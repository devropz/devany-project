{% extends '_layouts/base.njk' %}

{% block page_title %}Tour{% endblock %}

{% block content %}
<div class="container">
<br>
<br>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Date</th>
        <th>City</th>
        <th>Venue</th>
      </tr>
    </thead>
    {% for tourdates in tour.tourdates %}
    <tbody>
      <tr>
        <td>{{tourdates.date}}</td>
        <td>{{tourdates.city}}</td>
        <td>{{tourdates.venue}}</td>
      </tr>
    </tbody>
    {% endfor %}
  </table>
</div>
{% endblock %}